<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

	<form model="com.axelor.information.db.Person" title="Person"
		name="person-form" extension="true" id="person-form-two">
		
		<extend target="panel[@title='Input_Info']" >
			<insert position="after">
			 <panel-dashlet action="All.incharge" title="all info"/>
			</insert>
		</extend>
		
		<extend target="//field[@name='fullName']">
		<replace>
		<field name="name" required="true"/>
		</replace>
		</extend>
		
		<extend target="/">
		<attribute name="width" value="large"/>
		</extend>
		
		<extend target="panel[@sidebar='true']">
		<move position="after" source="panel[@title='Input_Info']"/>
		</extend>
	</form>

</object-views>
