<?xml version="1.0" encoding="UTF-8"?>


<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">


	<grid model="com.axelor.manufacturer.db.Address"
		title="All Manufactur Address" name="address-grid">

		<field name="street" />
		<field name="streetnumber" />
		<field name="city" />
		<field name="state" />
		<field name="country" />
	</grid>





	<form model="com.axelor.manufacturer.db.Address"
		title="Manufactur Address" name="address-form">
		<panel>
			<field name="street" />
			<field name="streetnumber" />
			<field name="city" />
			<field name="state" />
			<field name="country" />
		</panel>
		<panel-related field="contact" />
	</form>

</object-views>