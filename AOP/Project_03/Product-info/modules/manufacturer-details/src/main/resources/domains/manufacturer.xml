<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">
	<module package="com.axelor.manufacturer.db" name="manufacturer" />
	<entity name="Manufacturer">
		<string name="manufacturername" />
		<date name="dateofestablishment" />
		<time name="dispatchtime" />
		<integer name="gstnumber" />
		<one-to-many ref="Address" name="address" mappedBy="manufacturer" />
	</entity>

</domain-models>