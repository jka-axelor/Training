<?xml version="1.0" encoding="UTF-8"?>

<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">
	<module package="com.axelor.manufacturer.db" name="manufacturer" />
	<entity name="Address">
		<string name="street" />
		<integer name="streetnumber" />
		<string name="city" />
		<string name="state" />
		<string name="country" />
		<one-to-many ref="Contact" name="contact" mappedBy="address" />
		<many-to-one ref="Manufacturer" name="manufacturer"
			required="true" />
	</entity>

</domain-models>