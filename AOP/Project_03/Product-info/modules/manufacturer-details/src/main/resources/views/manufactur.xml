<?xml version="1.0" encoding="UTF-8"?>

<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

	<form model="com.axelor.manufacturer.db.Manufacturer"
		title="Manufacturs Details" name="manufactur-form">
		<panel name="mfd" title="Details" colSpan="24">
			<field name="manufacturername" colSpan="24"/>
			<field name="gstnumber" colSpan="24" />
			<field name="dateofestablishment" />
			<field name="dispatchtime" />
		</panel>
		<panel-related field="address" form-view="address-form" />
		
	</form>

	<grid model="com.axelor.manufacturer.db.Manufacturer"
		title="All Manufacturs" name="manufactur-grid">

		<field name="manufacturername" />
		<field name="gstnumber" />
		<field name="dateofestablishment" />
		<field name="dispatchtime" />
	</grid>

</object-views>