<?xml version="1.0" encoding="UTF-8"?>


<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

	<grid model="com.axelor.employees.db.Manager" title="All Manager"
		name="Manager-grid">
		<field name="title" />
		<field name="firstname" />
		<field name="middlename" />
		<field name="lastname" />
		<field name="dateofjoining" />
		<field name="phonenumber" />
		<field name="active" />
		<field name="workingtime" />
		<field name="description" />
		<field name="department" />
	</grid>

	<form model="com.axelor.employees.db.Manager" title="Manager"
		name="Manager-form" width="large">
		<panel name="Manager-panel" title="Manager-Details">
			<field name="title" selection="employess-prefix" />
			<field name="firstname" />
			<field name="middlename" />
			<field name="lastname" />
			<field name="dateofjoining" />
			<field name="phonenumber" />
			<field name="active" />
			<field name="workingtime" />
			<field name="description" />
			<field name="department" />
		</panel>

<panel-related field="hollydas"/>
<panel-related field="employess" height="10"/>
</form>
</object-views>





